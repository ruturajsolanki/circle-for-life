{
  "name": "circle-for-life-backend",
  "version": "1.0.0",
  "description": "Circle for Life - Create. Compete. Earn. Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:tsx": "npx tsx src/server.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "db:seed": "tsx src/scripts/seed.ts",
    "worker:image": "tsx src/workers/imageGeneration.worker.ts",
    "worker:moderation": "tsx src/workers/moderation.worker.ts",
    "worker:gems": "tsx src/workers/gemSettlement.worker.ts",
    "worker:analytics": "tsx src/workers/analytics.worker.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.0",
    "@fastify/jwt": "^9.0.0",
    "@fastify/multipart": "^9.0.0",
    "@fastify/rate-limit": "^10.2.0",
    "@fastify/websocket": "^11.0.0",
    "@supabase/supabase-js": "^2.95.3",
    "axios": "^1.7.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^5.30.0",
    "dayjs": "^1.11.0",
    "dotenv": "^16.4.0",
    "fastify": "^5.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.9.0",
    "ms": "^2.1.0",
    "nanoid": "^5.0.0",
    "pg": "^8.18.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.33.0",
    "tsx": "^4.19.0",
    "uuid": "^11.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "playwright": "^1.58.2",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
